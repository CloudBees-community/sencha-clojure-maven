<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<!--<listener>
	  <listener-class>com.cloudbees.sencha-backend.sencha.database</listener-class>
	</listener>-->
	<servlet>
          <servlet-name>myservlet</servlet-name>
          <servlet-class>simple.war</servlet-class>
        </servlet>
        <servlet-mapping>
          <servlet-name>myservlet</servlet-name>
          <url-pattern>/*</url-pattern>
        </servlet-mapping>
	<resource-ref>
	  <res-ref-name>comp/env/jdbc/ExampleDS</res-ref-name>
	  <!-- Comment the above line, and uncomment the following one to deploy on JBoss -->
	  <!-- <res-ref-name>jdbc/ExampleDS</res-ref-name> -->
	  <res-type>javax.sql.DataSource</res-type>
	  <res-auth>Container</res-auth>
  	</resource-ref>
</web-app>